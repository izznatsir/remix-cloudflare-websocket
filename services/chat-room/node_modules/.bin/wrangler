#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules/wrangler/bin/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules/wrangler/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules/wrangler/bin/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules/wrangler/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/wrangler@3.28.0_@cloudflare+workers-types@4.20240208.0/node_modules:/home/izznatsir/codes/github.com/izznatsir/remix-cloudflare-chat-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../wrangler/bin/wrangler.js" "$@"
else
  exec node  "$basedir/../wrangler/bin/wrangler.js" "$@"
fi
